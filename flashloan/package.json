{
  "name": "hardhat-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate repository to get you started with Hardhat and Ethereum development",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NomicFoundation/hardhat-boilerplate.git"
  },
  "author": "Nomic Foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NomicFoundation/hardhat-boilerplate/issues"
  },
  "homepage": "https://github.com/NomicFoundation/hardhat-boilerplate#readme",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "lint": "npx solhint contracts/*.sol && npx eslint . && npx markdownlint --ignore node_modules **/*.md",
    "format": "prettier --check \"contracts/**/*.sol\" \"**/*.ts\" \"**/*.md\" \"**/*.yml\"",
    "test": "npx hardhat test",
    "coverage": "npm run build && npx hardhat coverage --temp artifacts --network coverage"
  },
  "dependencies": {
    "@aave/core-v3": "^1.18.0",
    "@ensdomains/ens": "^0.4.4",
    "@ensdomains/resolver": "^0.2.4",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@openzeppelin/contracts": "^4.9.3",
    "@typechain/hardhat": "^9.0.0",
    "@types/sinon-chai": "^3.2.10",
    "@uniswap/v3-periphery": "^1.4.4",
    "axios": "^1.5.1",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "ethereum-waffle": "^4.0.10",
    "invariant": "^2.2.4",
    "jsbi": "^4.3.0",
    "prettier": "^3.0.3",
    "prettier-plugin-solidity": "^1.1.3",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.9",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@typechain/ethers-v5": "^11.1.1",
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "chai": "^4.3.10",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "ethers": "^6",
    "hardhat": "^2.17.4",
    "hardhat-deploy": "^0.11.37",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-typechain": "^0.3.5",
    "markdownlint": "^0.31.1",
    "markdownlint-cli": "^0.37.0",
    "solidity-coverage": "^0.8.5"
  }
}
